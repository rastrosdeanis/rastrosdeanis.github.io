@font-face {
  font-family: "Crimson";
  src: url("/assets/fonts/CrimsonText-Regular.ttf") format("truetype");
  font-style: normal;
  font-weight: 400;
}

@font-face {
  font-family: "Crimson";
  src: url("/assets/fonts/CrimsonText-Italic.ttf") format("truetype");
  font-style: italic;
  font-weight: 400;
}

body {
  margin: 0;
  padding: 0;
  font-family: "Crimson", serif !important;
  color: #e6e6e6;
  background: #0e0e0e;
}
